{
  "name": "kachicken",
  "version": "1.0.0",
  "description": "Meal planning sharing app",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "build": "npm run clean && npm run build:webpack && npm run copy:html && npm run imagemin && npm run copy:assets && npm run copy:fonts",
    "build:webpack": "webpack ",
    "clean": "rimraf dist",
    "copy:html": "copyfiles -f ./*.html dist",
    "copy:assets": "copyfiles -f ./assets/coiny-v4-latin/* dist/assets/coiny-v4-latin",
    "copy:fonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/css/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server -c ./bs-config.js",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "npm run lite"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "grunt": "^1.0.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-htmlmin": "^3.0.0",
    "grunt-contrib-imagemin": "^3.1.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^3.0.2",
    "grunt-usemin": "^3.1.1",
    "grunt-webpack": "^3.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.3.0",
    "load-grunt-tasks": "^4.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "time-grunt": "^2.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  }
}
